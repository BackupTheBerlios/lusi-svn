<!ELEMENT ul (li*)>
<!ATTLIST ul id CDATA #FIXED "screenshotsList"
             xmlns CDATA #FIXED "http://www.w3.org/1999/xhtml">

<!ELEMENT li (p, a)>

<!ELEMENT p (#PCDATA)>

<!ELEMENT a (img)>
