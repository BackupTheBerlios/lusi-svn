<?xml version="1.0" encoding="utf-8"?>
<page xmlns="http://lusi.berlios.de/namespaces/page" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="page.xsd">
	<variables>
		<pageId>All news</pageId>
		<headElements>
			<link xmlns="http://www.w3.org/1999/xhtml" rel="alternate" type="application/atom+xml" href="feed/atom/news.xml" title="Atom feed for LUSI project news"/>
		</headElements>
		<validationButtons>
			<p xmlns="http://www.w3.org/1999/xhtml">
				<a href="http://feedvalidator.org/check.cgi?url=http%3A//lusi.berlios.de/feed/atom/news.xml" title="Check Atom feed">
					<img src="images/valid-atom.png" alt="Valid Atom 1.0!" height="15" width="80"/>
				</a>
			</p>
		</validationButtons>
	</variables>
	<page:content xmlns:page="http://lusi.berlios.de/namespaces/page" xmlns="http://www.w3.org/1999/xhtml">
